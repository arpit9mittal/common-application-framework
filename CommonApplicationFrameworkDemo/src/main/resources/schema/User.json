{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"id": {
			"$id": "/properties/id",
			"type": "integer"
		},
		"username": {
			"$id": "/properties/username",
			"type": "string",
			"minLength": 5,
			"maxLength": 200
		},
		"firstName": {
			"$id": "/properties/firstName",
			"type": "string",
			"minLength": 2,
			"maxLength": 100
		},
		"lastName": {
			"$id": "/properties/lastName",
			"type": "string",
			"minLength": 2,
			"maxLength": 250
		}
	},
	"required": [
		"id",
		"username",
		"firstName",
		"lastName"
	]
}